{
  "name": "front-end",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.2",
    "@mui/lab": "^5.0.0-alpha.82",
    "@mui/material": "^5.6.3",
    "@mui/system": "^5.6.3",
    "@vitejs/plugin-react": "^2.2.0",
    "axios": "^1.2.0",
    "formik": "^2.2.9",
    "lodash": "^4.17.21",
    "material-ui-confirm": "^3.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-router-dom": "^6.4.4",
    "react-textarea-autosize": "^8.4.0",
    "react-transition-group": "^4.4.5",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "yup": "^0.32.11"
  },
  "main": "./src/main.jsx",
  "scripts": {
    "start-backend": "cd ./server && yarn start",
    "start-dev-backend": "cd ./server && yarn dev",
    "start-react": "wait-on tcp:80 && yarn vite",
    "start": "yarn concurrently --kill-others \"yarn start-react\" \"yarn start-backend\"",
    "dev": "set \"REACT_MODE=1\" && yarn concurrently --kill-others  \"yarn start-react\" \"yarn start-dev-backend\" && cls",
    "build": "vite build --mode production && cd ./server && yarn esbuild",
    "eslint": "eslint --ext .js,.ts --ignore-pattern build/**/*,*.test **/*"
  },
  "proxy": "http://127.0.0.1:80",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.32",
    "@types/react": "^18.0.6",
    "@types/react-dom": "^18.0.2",
    "@types/react-transition-group": "^4.4.4",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "concurrently": "^7.1.0",
    "eslint": "^8.34.0",
    "eslint-plugin-react": "^7.32.2",
    "ts-node": "^10.8.0",
    "wait-on": "^6.0.1"
  }
}
